{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "review",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "objectPosition": {
      "type": "string",
      "required": true,
      "default": "50% 50%",
      "regex": "(\\d{1,3}%)\\s(\\d{1,3}%)"
    },
    "reviewContent": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "blocks",
      "required": true
    }
  }
}
